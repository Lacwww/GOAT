<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<typeAliases>
		<typeAlias alias="place" type="com.ch.goat.model.Place" />
		<typeAlias alias="placereview"
			type="com.ch.goat.model.PlaceReview" />
		<typeAlias alias="bookmark"
			type="com.ch.goat.model.Bookmark" />
		<typeAlias alias="tempplace"
			type="com.ch.goat.model.TempPlace" />
		<typeAlias alias="member" type="com.ch.goat.model.Member" />
		<typeAlias alias="trip" type="com.ch.goat.model.Trip" />
		<typeAlias alias="tl" type="com.ch.goat.model.TripLike" />
		<typeAlias alias="tr" type="com.ch.goat.model.TripReply" />

		<typeAlias alias="notice" type="com.ch.goat.model.Notice" />
		<typeAlias alias="nr" type="com.ch.goat.model.NoticeReply" />
		
		<typeAlias alias="cs" type="com.ch.goat.model.Cs" />
		
		<typeAlias alias="sc" type="com.ch.goat.model.Schedule" />
		<typeAlias alias="area" type="com.ch.goat.model.Area" />
		<typeAlias alias="scd"
			type="com.ch.goat.model.ScheduleDetail" />
		<typeAlias alias="alert" type="com.ch.goat.model.Alert"/>

	</typeAliases>
	<mappers>
		<mapper resource="place.xml" />
		<mapper resource="schedule.xml" />
		<mapper resource="member.xml" />
		<mapper resource="trip.xml" />
		<mapper resource="notice.xml" />
		<mapper resource="noticeReply.xml" />
		<mapper resource="cs.xml" />
		<mapper resource="admin.xml" /> 
	</mappers>
</configuration>